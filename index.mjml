<mjml>
  <mj-head>
    <mj-style>
      * {
        box-sizing: border-box;
      }
    </mj-style>

    <!-- The inline style will be used for reusability throughout the email. -->
    <mj-style inline="inline">
      .header-link {
        text-decoration: none;
        color: #e0164a;
        font-weight: bold;
        font-size: 14px;
        margin: 0;
        white-space: nowrap;
      }

      .shop-now-button {
        text-decoration: none;
        background-color: #e0164a;
        padding: 12px 15px;
        font-weight: bold;
        color: white;
        border-radius: 5px;
        margin: 0px;
      }

      .recommendation-title {
        text-decoration: none;
        color: #555555;
        font-weight: 300;
      }
    </mj-style>
  </mj-head>
  <mj-body background-color="#f2f2f2">

    <mj-wrapper background-color="white">
      <!-- Header -->
      <mj-include path="./components/header.mjml" />
      <!-- End of Header -->

      <!-- Start of Divider -->
      <mj-include path="./components/divider.mjml" />
      <!-- End of Divider -->

      <mj-section padding="5px 0 20px">
        <mj-column>
          <mj-text align="center" font-size="20px" font-weight="bold" letter-spacing="1.2px" color="#555555" padding="0">
            Shop the Soccer Ball Sale!
          </mj-text>
        </mj-column>
      </mj-section>

      <mj-section padding="0">
        <mj-column>
          <mj-image src="/assets/banner.jpg" alt="" />
        </mj-column>
      </mj-section>

      <!-- Start of Divider -->
      <mj-include path="./components/divider.mjml" />
      <!-- End of Divider -->

      <!-- New FootWear Section -->
      <mj-section padding="10px 0px 20px">
        <mj-column>
          <mj-text align="center" font-size="20px" font-weight="bold" letter-spacing="1.2px" color="#555555" padding="0">
            Shop New Footwear
          </mj-text>
        </mj-column>
      </mj-section>


      <mj-section padding="0px 10px 25px">
        <mj-column padding="0px">
          <mj-image src="/assets/cleats/cleat-1.jpg" alt="Blue Cleats" padding="0 8px" href="https://e.email.soccer.com/click?" />
          <mj-text align="center">
            <a href="https://e.email.soccer.com/click?" class="shop-now-button">Shop Now</a>
          </mj-text>
        </mj-column>

        <mj-column padding="0px">
          <mj-image src="/assets/cleats/cleat-2.jpg" alt="white cleats" padding="0 8px" href="https://e.email.soccer.com/click?" />
          <mj-text align="center">
            <a href="https://e.email.soccer.com/click?" class="shop-now-button">Shop Now</a>
          </mj-text>
        </mj-column>

        <mj-column padding="0px">
          <mj-image src="/assets/cleats/cleat-3.jpg" alt="red cleats" padding="0 8px" href="https://e.email.soccer.com/click?" />
          <mj-text align="center">
            <a href="https://e.email.soccer.com/click?" class="shop-now-button">Shop Now</a>
          </mj-text>
        </mj-column>
      </mj-section>

      <!-- End of Footwear Section -->

      <!-- Start of Divider -->
      <mj-include path="./components/divider.mjml" />
      <!-- End of Divider -->

      <!-- Top Selling Section -->
      <mj-section padding="10px 0px 20px">
        <mj-column>
          <mj-text align="center" font-size="20px" font-weight="bold" letter-spacing="1.2px" color="#555555" padding="0">
            Shop Top Selling Footwear
          </mj-text>
        </mj-column>
      </mj-section>


      <mj-section padding="0px 10px 25px">
        <mj-column padding="0px">
          <mj-image src="/assets/cleats/cleat-4.jpg" alt="white cleats" padding="0 8px" href="https://e.email.soccer.com/click?" />
          <mj-text align="center">
            <a href="https://e.email.soccer.com/click?" class="shop-now-button">Shop Now</a>
          </mj-text>
        </mj-column>

        <mj-column padding="0px">
          <mj-image src="/assets/cleats/cleat-5.jpg" alt="yellow cleats" padding="0 8px" href="https://e.email.soccer.com/click?" />
          <mj-text align="center">
            <a href="https://e.email.soccer.com/click?" class="shop-now-button">Shop Now</a>
          </mj-text>
        </mj-column>

        <mj-column padding="0px">
          <mj-image src="/assets/cleats/cleat-9.jpg" alt="" padding="0 8px" href="https://www.soccer.com/shop/details/nike-zoom-mercurial-vapor-16-elite-cosmic-speed-fg-firm-ground-soccer-cleat-black-black_A1103254" />
          <mj-text align="center">
            <a href="https://www.soccer.com/shop/details/nike-zoom-mercurial-vapor-16-elite-cosmic-speed-fg-firm-ground-soccer-cleat-black-black_A1103254" class="shop-now-button">Shop Now</a>
          </mj-text>
        </mj-column>
      </mj-section>
      <!-- End of Top Selling Section -->

      <!-- Start of Divider -->
      <mj-include path="./components/divider.mjml" />
      <!-- End of Divider -->

      <!-- Reccomendations Section -->
      <mj-section padding="10px 0px 20px">
        <mj-column>
          <mj-text align="center" font-size="20px" font-weight="bold" letter-spacing="1.2px" color="#555555" padding="0">
            Your Footwear Recommendations
          </mj-text>
        </mj-column>
      </mj-section>


      <mj-section padding="0px 40px 5px">
        <mj-column padding="0px">
          <mj-image src="/assets/cleats/cleat-6.jpg" alt="" padding="0 8px" href="https://e.email.soccer.com/click?" />
          <mj-text align="center" padding="0 5px">
            <a class="recommendation-title" href="https://e.email.soccer.com/click?">adidas F50 Elite Laceless FG Firm Ground Soccer Cleat Almost Blue/Semi Lucid Blue/Semi Solar Yellow</a>
          </mj-text>
        </mj-column>

        <mj-column padding="0px">
          <mj-image src="/assets/cleats/cleat-7.jpg" alt="" padding="0 8px" href="https://e.email.soccer.com/click?" />
          <mj-text align="center" padding="0 5px">
            <a class="recommendation-title" href="https://e.email.soccer.com/click?" class="">Nike Zoom Mercurial Vapor 15 Elite FG Soccer Cleat Lemonade/Black</a>
          </mj-text>
        </mj-column>
        <mj-column padding="0px">
          <mj-image src="/assets/cleats/cleat-8.jpg" alt="" padding="0 8px" href="https://www.soccer.com/shop/details/nike-zoom-mercurial-vapor-16-elite-cosmic-speed-fg-firm-ground-soccer-cleat-black-black_A1103254" />
          <mj-text align="center" padding="0 5px">
            <a class="recommendation-title" href="https://www.soccer.com/shop/details/nike-zoom-mercurial-vapor-16-elite-cosmic-speed-fg-firm-ground-soccer-cleat-black-black_A1103254" class="">Nike Zoom Mercurial Superfly 10 Elite FG Firm Ground Soccer Cleat White/Racer Blue</a>
          </mj-text>
        </mj-column>
      </mj-section>
      <!-- End of Recommendations Section -->


      <!-- Start of Divider -->
      <mj-include path="./components/divider.mjml" />
      <!-- End of Divider -->


      <mj-section padding="5px 0 20px">
        <mj-column>
          <mj-text align="center" font-size="20px" font-weight="bold" letter-spacing="1.2px" color="#555555" padding="0">
            Shop the Soccer Ball Sale!
          </mj-text>
        </mj-column>
      </mj-section>

      <mj-section padding="0">
        <mj-column>
          <mj-image src="/assets/discount.jpg" alt="" />
        </mj-column>
      </mj-section>


      <!-- Start of Divider -->
      <mj-include path="./components/divider.mjml" />
      <!-- End of Divider -->



    </mj-wrapper>
  </mj-body>
</mjml>









































